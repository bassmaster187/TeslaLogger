          "group": [],
          "metricColumn": "none",
          "rawQuery": true,
          "rawSql": "SELECT \n  lat, \n  lng, \n  EndLat, \n  EndLng, \n  UNIX_TIMESTAMP(StartDate)*1000 as StartDate, \n  UNIX_TIMESTAMP(EndDate)*1000 as EndDate,  \n  Start_address, \n  End_address, \n  avg_consumption_kWh_100km, \n  consumption_kWh, \n  DurationMinutes, \n  km_diff, StartRange - EndRange as RangeDiff,\n  (CASE \n    WHEN StartRange - EndRange > km_diff*1.2 THEN '⬇️'\n    WHEN StartRange - EndRange > km_diff*1.1 THEN '↘️'\n    WHEN StartRange - EndRange < km_diff*0.9 THEN '↗️'\n    WHEN StartRange - EndRange < km_diff*0.8 THEN '⬆️'\n    ELSE '➡️'\n  END) AS ↕️,  \n  outside_temp_avg, \n  speed_max,power_max/1.35962 as power_max,\n  power_min/1.35962 as power_min,\n  power_avg/1.35962 as power_avg, \n  (select km_diff / DurationMinutes * 60) as avg_kmh, \n  startposid, \n  endposid, \n  'GPX'\nFROM \n  trip \nWHERE \n  $__timeFilter(StartDate) \n  AND km_diff < 1000 \n  AND CarID = $Car \n  AND (Start_address like '%$Textfilter%' OR End_address like '%$Textfilter%')\nORDER BY \n  StartDate DESC\n",
